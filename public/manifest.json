{
  "name": "LinkedIn Connect All",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/mynetwork/grow/"],
      "js": ["contentScript.js"]
    }
  ]
}
